<h1>List of users</h1>



<hr>

<table border="1">
  <tr>
    <th>Private?</th>
    <th>Username</th>
    <td></td>
  </tr>

  <% @list_of_users.each do |a_user| %>
    <tr>
      <td><%= a_user.private ? "Yes" : "No" %></td>
      <td><%= a_user.username %></td>
      <td><%= link_to "Show details", user_path(a_user.username) %></td>
    </tr>
  <% end %>
</table>
